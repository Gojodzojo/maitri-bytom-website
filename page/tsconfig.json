{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$components/*": [
        "src/components/*"
      ],
      "$web_components/*": [
        "../web_components/src/*"
      ],
      "$layouts/*": [
        "src/layouts/*"
      ],
      "$scripts/*": [
        "src/scripts/*"
      ],
      "$styles/*": [
        "src/styles/*"
      ]
    }
  }
}